<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<!-- Google Fonts -->
<link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900' rel='stylesheet' type='text/css'>
<!-- Google Fonts -->
<script type="text/javascript" src="js/move-top.js"></script>
<script type="text/javascript" src="js/easing.js"></script>
<script type="text/javascript">
jQuery(document).ready(function($) {
	$(".scroll").click(function(event){		
		event.preventDefault();
		$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
	});
});
</script>
</head>
<body>
	<!-- Header Starts Here -->
	<%- include ("./header") %>
	<!-- Header Ends Here -->
	<!-- Save Starts Here -->
	<div class="save" id="save">
		<div class="container">
			<div class="reviews" id="review">
				<div class="container">
					<a href="#"><img src="images/logo_small.png" width="160px" height="50px" alt=""></a>
				</br></br>
					<div  id="top" class="callbacks_container">
						  <ul class="rslides" id="slider4">
							<% for(var i=0;i< comand.length;i++){%>
								<% if (comand.length!==0){%>
									
									<li>
								
										<div class="slider-top">
											<table>
												
											<td><a ><img src="<%=comand[i].Mobile.filepath%>" width="100" height="100"></a></td>
											<tr> 
											
											<td id="<%=i%>"><p><%=comand[i].Mobile.mobile%></p></td>
											</tr>
											<tr>
												<form action="anuler"	method="POST">
												<td><input class="rev" name="id" id="id" value="<%=comand[i].id%>" hidden></td>
												<tr>
											<tr>
											<td><p class="rev"><%=comand[i].Mobile.prix%></p></td>
											<tr>
											<td><input type="submit" class="contact" value="annuler" ></td>
										</tr>
											</tr>
										</form>
											</table>
										</div>
									</li>
									
								
							<%}%>
							<%}%>
						
						  </ul>
					</div>
		
				</div>
			</div>
			<!-- Reviews Ends Here -->
		</div>
	</div>
	<!-- Save Ends Here -->
	
	<!-- Reviews Starts Here -->
		<script>
		    // You can also use "$(window).load(function() {"
		    $(function () {
		      // Slideshow 4
		      $("#slider1").responsiveSlides({
		        auto: true,
		        pager: true,
		        nav: true,
		        speed: 500,
		        namespace: "callbacks",
